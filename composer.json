{
    "name": "devua/salesteer-laravel",
    "description": "Salesteer PHP Library",
    "keywords": [
        "salesteer",
        "crm",
        "api"
    ],
    "homepage": "https://salesteer.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Salesteer and contributors",
            "homepage": "https://github.com/devua-git/salesteer-laravel/contributors"
        }
    ],
    "require": {
        "php": "*",
        "ext-json": "*",
        "illuminate/contracts": ">=11.0",
        "illuminate/http": ">=11.0",
        "illuminate/log": ">=11.0",
        "illuminate/pagination": ">=11.0",
        "illuminate/routing": ">=11.0",
        "illuminate/support": ">=11.0",
        "nesbot/carbon": ">=3.0",
        "devua/salesteer-php-sdk": "<2.0.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Salesteer\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "extra": {},
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}